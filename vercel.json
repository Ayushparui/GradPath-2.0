{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "rewrites":[
        { "source": "/(.*)", "destination": "/" }
    ],
    "env": {
      "NODE_ENV": "production",
      "API_URL": "https://grad-path-2-0.vercel.app"
    },
    "build": {
      "env": {
        "NODE_ENV": "production",
        "API_URL": "https://grad-path-2-0.vercel.app"
      }
    }
  }
  